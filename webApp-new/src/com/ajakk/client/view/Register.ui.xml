<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
 xmlns:ma="urn:import:gwt.material.design.addins.client">
 <ui:style>
 .btn {
 	margin: auto;
 	width: 100%;
 	height: 52px;
 	margin-bottom: 3px;
 	font-family: Ionicons;
 	font-size: 20px;
 	color: #000000;
 	text-shadow: 0 2px 4px rgba(0, 0, 0, 0.50);
 }
 
 .lggbtn {
 	font-family: Ionicons;
 	font-size: 20px;
 	color: #000000;
 	text-shadow: 0 2px 4px rgba(0, 0, 0, 0.50);
 	margin: auto;
 	width: 100%;
 	height: 52px;
 	margin-bottom: 3px;
 }
 
 .lgfbtn {
 	margin: auto;
 	width: 100%;
 	height: 52px;
 	margin-bottom: 3px;
 	font-family: simple-line-icons;
 	font-size: 20px;
 	color: #FFFFFF;
 	text-shadow: 0 2px 4px rgba(0, 0, 0, 0.50);
 }
 </ui:style>
 <m:MaterialModal ui:field="modal" type="DEFAULT" inDuration="200" outDuration="200">
  <m:MaterialModalContent>
   <m:MaterialLabel textAlign="CENTER">Sign up with Ajakk</m:MaterialLabel>
   <m:MaterialTextBox ui:field="txtName" label="Name" iconType="WORK" />
   <m:MaterialTextBox ui:field="txtEmail" label="Email" iconType="ACCOUNT_CIRCLE" />
   <m:MaterialTextBox ui:field="txtPassword" label="Password" iconType="SECURITY" />
   <m:MaterialTextBox ui:field="txtPhone" label="Phone" iconType="SMARTPHONE" />
   <m:MaterialTextBox ui:field="txtLocation" label="Location" iconType="PLACE" />
   <m:MaterialButton ui:field="btnReg" text="REGISTER" waves="RED" backgroundColor="BLUE" textColor="WHITE" iconType="CREATE"
    iconPosition="LEFT" display="BLOCK" addStyleNames="{style.btn}">
   </m:MaterialButton>
   <!-- <m:MaterialButton ui:field="btnRegGmail" text="SIGN UP VIA GOOGLE+" waves="RED" backgroundColor="RED" textColor="WHITE" iconType="POLYMER" 
    iconPosition="LEFT" display="BLOCK" addStyleNames="{style.btn}"> </m:MaterialButton> <m:MaterialButton ui:field="btnRegFb" text="SIGN UP VIA 
    FACEBOOK" waves="RED" backgroundColor="INDIGO_DARKEN_4" textColor="WHITE" iconType="POLYMER" iconPosition="LEFT" display="BLOCK" addStyleNames="{style.btn}"> 
    </m:MaterialButton> -->
  </m:MaterialModalContent>
 </m:MaterialModal>
</ui:UiBinder> 