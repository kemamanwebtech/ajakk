<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
 xmlns:ma="urn:import:gwt.material.design.addins.client">
  <ui:style>
	.modal {
		display: inline-block;
		text-align: center;
		font-size: 150%;
		box-shadow: none;
	}
 </ui:style>

<m:MaterialModal ui:field="modal" addStyleNames="{style.modal}" backgroundColor="TRANSPARENT">
	<m:MaterialModalContent>
		<m:MaterialRow>
			<m:MaterialIcon iconType="ACCOUNT_CIRCLE" iconSize="LARGE"></m:MaterialIcon>
			<m:MaterialLabel text="Rafsyam"></m:MaterialLabel>
		</m:MaterialRow>
		<m:MaterialLabel text="Basketballer"></m:MaterialLabel>
		<m:MaterialLabel text="Cyberjaya"></m:MaterialLabel>
		<m:MaterialLabel text="Joined since 2018"></m:MaterialLabel>
		<m:MaterialLink iconSize="SMALL" iconType="EDIT" iconPosition="RIGHT" />
	 </m:MaterialModalContent>
</m:MaterialModal>
 
</ui:UiBinder> 